{
	"name": "pdp-20",
	"version": "1.0.0",
	"description": "First package for learning",
	"scripts": {
		"sass:home": "sass --watch ./src/styles/pages/home/home.scss:./dist/styles/home.css",
		"sass:about": "sass --watch ./src/styles/pages/about/about.scss:./dist/styles/about.css",
		"sass:build:home": "sass --no-source-map --style compressed ./src/styles/pages/home/home.scss:./dist/styles/home.css",
		"sass:build:about": "sass --no-source-map --style compressed ./src/styles/pages/about/about.scss:./dist/styles/about.css",
		"sass:build:contact": "sass --no-source-map --style compressed ./src/styles/pages/about/about.scss:./dist/styles/about.css",
		"sass:build": "bun sass:build:home && bun sass:build:about && bun sass:build:contact"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PDP-FRONTEND/PDP-G-20.git"
	},
	"keywords": [
		"sass",
		"js",
		"pdp",
		"html",
		"css"
	],
	"author": "G-20",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/PDP-FRONTEND/PDP-G-20/issues"
	},
	"homepage": "https://github.com/PDP-FRONTEND/PDP-G-20#readme"
}
